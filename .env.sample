# RemBG CV Maker Environment Variables
# Copy this file to .env and fill in your actual values

# RemBG API Key (Required)
# Get your API key from https://www.rembg.com
REMBG_API_KEY=your_rembg_api_key_here

# Server Port (Optional)
# Default: 3000
PORT=3000

# Example:
# REMBG_API_KEY=rembg_1234567890abcdef
# PORT=3000
